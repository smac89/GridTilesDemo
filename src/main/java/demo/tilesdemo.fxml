<?xml version="1.0" encoding="UTF-8"?>

<?import eu.hansolo.tilesfx.Tile?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" stylesheets="@tilesdemo.css" xmlns="http://javafx.com/javafx/8.0.111"
          fx:controller="demo.TilesDemoPresenter">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Tile fx:id="mSomeMediaView" skinType="CUSTOM" title="NICE" GridPane.columnSpan="2" GridPane.rowSpan="2"/>
        <ListView fx:id="mListOfStuff" maxHeight="-Infinity" maxWidth="-Infinity" GridPane.columnSpan="2"
                  GridPane.rowIndex="2"/>
    </children>
</GridPane>
